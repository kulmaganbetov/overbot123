generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Message {
  id        Int      @id @default(autoincrement())
  role      String
  content   String
  createdAt DateTime @default(now())
  sessionId String?
}

model SessionBuild {
  id         Int      @id @default(autoincrement())
  sessionId  String   @unique
  data       Json     @default("{}")
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}